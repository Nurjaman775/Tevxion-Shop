{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/web/:path*", "destination": "/web/:path*" },
    { "source": "/login/:path*", "destination": "/login/:path*" },
    { "source": "/register/:path*", "destination": "/register/:path*" },
    { "source": "/dashboard/:path*", "destination": "/dashboard/:path*" },
    { "source": "/toko-belanja/:path*", "destination": "/toko-belanja/:path*" },
    { "source": "/(.*)", "destination": "/web/index.html" }
  ]
}
